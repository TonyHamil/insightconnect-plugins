#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from icon_kace import connection, actions, triggers


Name = 'KACE'
Vendor = 'quest'
Version = '1.0.6'
Description = 'Work with KACE SMA'


class ICONKace(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name, 
                vendor=Vendor, 
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_action(actions.LookupDevice())

        self.add_action(actions.SystemRefresh())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONKace())
    cli.run()


if __name__ == "__main__":
    main()
